  internal_properties : CAAAAAgAAADSlU3WzSuKhwzvv9ekap4aRWvQyiozkIrtX8LBztyizfg6bHlgyNnk0D+cZAltaS+u8ZX8iVBsCoGBqN21e+j7Idlgc/0kfO/B5Q2U
  type_code : 0
  p_codes :
   -
     internal_properties : 
     code : |1+
      
      HCreationIfNotFound("*")
      
      gnIDBlog is int
      gnIDMembre is int
      gsSession is string
      
      // Blog en cours
      gnIDBlog = Val(PageParameter("blog"))
      
      // Login du membre
      local
      	if PageParameter("do") = "bye" then
      		// Supprime la session en cours
      		CookieWrite("session", "")
      		gsSession = ""
      		gnIDMembre = 0
      	else
      		
      		sSession is string
      		gsSession = CookieRead("session")
      		sSession = gsSession
      		
      		if sSession = "" then
      			// L'utilisateur n'est pas enregistré
      			gnIDMembre = 0
      		else
      			sLogin is string
      			sLogin = ExtractString(sSession, 2, "-")
      			
      			HReadSeekFirst(Membre,Pseudo,slogin)
      			if HFound(Membre) then
      				
      				sSession = Decrypt(ExtractString(sSession, 1, "-"), "awp",cryptNone,True)
      				if Val(ExtractString(sSession, 2)) = membre.idmembre then
      					
      					// Extrait la date et heure du début de session
      					dDateHeureDébutSession is datetime = ExtractString(sSession, 1)
      					dDateHeureEnCours is datetime = Today + TimeSys
      					
      					// Si la session a plus d'une journée
      					if Val(Left(DateTimeDifference(dDateHeureDébutSession, dDateHeureEnCours),8)) > 0 then
      						// Sauvegarde
      						CookieWrite("session", "")				
      						// Cela fait plus d'un jour, la session a expiré
      						gnIDMembre = 0
      						// Il faut se logger
      						ScriptDisplay("login.awp?reason=expire&next=" + URLDecode(PageParameter("next")))
      					else
      						// On actualise la session
      						GenèreSession()					
      						gnIDMembre = membre.IDMembre
      						membre.session = gsSession
      						HModify(Membre)
      						
      						// Sauvegarde
      						CookieWrite("session", gsSession, 1)
      					end
      				else
      					// Sauvegarde
      					CookieWrite("session", "")				
      					gnIDMembre = 0	
      					ScriptDisplay("login.awp?reason=invalid&next=" + URLDecode(PageParameter("next")))			
      				end
      			else
      				// Sauvegarde
      				CookieWrite("session", "")				
      				gnIDMembre = 0	
      				ScriptDisplay("login.awp?reason=notfound&next=" + URLDecode(PageParameter("next")))
      			end
      		end		
      		
      	end	
      	
      
